{
  "name": "kube-ldap-client-go-exec-plugin",
  "version": "0.0.1",
  "description": "client-go-exec plugin for kube-ldap",
  "main": "src/index.js",
  "author": "Fabian.jucker <jucker@gyselroth.com>",
  "license": "MIT",
  "devDependencies": {
    "btoa": "^1.2.1",
    "commander": "^2.20.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "jest": "^24.8.0",
    "jsonwebtoken": "^8.2.0",
    "nexe": "^3.3.0",
    "node-fetch": "^2.1.2"
  },
  "scripts": {
    "build": "yarn run nexe -o build/kube-ldap-client-go-exec-plugin_$npm_package_version",
    "build:linux": "yarn run nexe -o build/kube-ldap-client-go-exec-plugin_linux-$npm_package_version",
    "build:osx": "yarn run nexe -o build/kube-ldap-client-go-exec-plugin_osx_$npm-package_version",
    "build:windows": "yarn run nexe -o build/kube-ldap-client-go-exec-plugin_windows-$npm_package_version.exe",
    "lint": "yarn run eslint src/",
    "test": "yarn run jest test/"
  }
}
